{
  "name": "vscode-homepage-extension",
  "displayName": "VS Code Homepage Extension",
  "description": "An extension that provides an improved home page experience within Visual Studio Code.",
  "version": "0.1.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:extension.showHomePage"
  ],
  "main": "./src/extension.ts",
  "contributes": {
    "commands": [
      {
        "command": "extension.showHomePage",
        "title": "Show Home Page"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./tsconfig.json",
    "watch": "tsc -watch -p ./tsconfig.json",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^4.4.3",
    "vscode": "^1.1.37"
  },
  "dependencies": {}
}