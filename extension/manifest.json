{
  "manifest_version": 3,
  "name": "Elegant Dashboard - New Tab",
  "version": "1.0.0",
  "description": "A beautiful, modern dashboard for your new tab page with customizable widgets and elegant design",
  "permissions": [
    "storage",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Elegant Dashboard Settings"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
